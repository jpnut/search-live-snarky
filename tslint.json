{
    "defaultSeverity": "error",
    "extends": [
        "tslint:latest",
        "tslint-react",
        "tslint-eslint-rules",
        "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {
        "interface-name": [
            true,
            "never-prefix"
        ],
        "no-console": false,
        "no-implicit-dependencies": [
            true,
            "dev"
        ],
        "no-submodule-imports": false
    },
    "rulesDirectory": []
}